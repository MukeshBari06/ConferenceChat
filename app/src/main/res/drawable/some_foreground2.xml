<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="10dp"
                android:color="@color/bgcolor"/>

        </shape>
    </item>
    <item
        android:left="2dp"
        android:top="2dp"
        android:bottom="2dp"
        android:right="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="20dp" />
            <stroke
                android:width="11dp"
                android:color="@color/bgcolor"/>
        </shape>
    </item>


</layer-list>